<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>dontprint@robamler.github.com</em:id>
		<em:name>Dontprint (paperless printer)</em:name>
		<em:version>0.2</em:version>
		<em:description>Send scientific articles from journal websites to your Amazon Kindle with a single click.</em:description>
		<em:creator>Robert Bamler</em:creator>
		<em:homepageURL>https://github.com/robamler/dontprint</em:homepageURL>
		<em:updateURL></em:updateURL> <!-- TODO -->
		<em:type>2</em:type> <!-- type=extension -->
		<em:optionsURL>chrome://dontprint/content/options.xul</em:optionsURL>
		
		<!-- Requires Firefox (Zotero Standalone not yet supported) -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>3.6</em:minVersion>
				<em:maxVersion>12.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>
</RDF>
